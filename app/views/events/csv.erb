Name, First Name, Last Name, Email, Status, Badge
<% @event.attendees.each do |attendee| %>
<%= "#{attendee.first_name} #{attendee.last_name}, #{attendee.first_name},#{attendee.last_name}, #{attendee.email}, #{attendee.status_types[attendee.status]}, #{attendee.badge_types[attendee.badge]}" %>
<% end %>
